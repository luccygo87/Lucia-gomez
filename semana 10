class Ciudadano:
    def __init__(self, nombre, vacunado=False, vacuna=""):
        self.nombre = nombre
        self.vacunado = vacunado
        self.vacuna = vacuna

    def __hash__(self):
        return hash(self.nombre)

    def __eq__(self, other):
        if isinstance(other, Ciudadano):
            return self.nombre == other.nombre
        return False

    def __repr__(self):
        return f"{self.nombre} - Vacunado: {self.vacunado} - Vacuna: {self.vacuna}"

def generar_ciudadanos(cantidad):
    return {Ciudadano(f"Ciudadano {i}") for i in range(1, cantidad + 1)}

def generar_vacunados(vacuna, cantidad):
    return {Ciudadano(f"Vacunado {vacuna} {i}", vacunado=True, vacuna=vacuna) for i in range(1, cantidad + 1)}

def obtener_no_vacunados(ciudadanos):
    return {ciudadano for ciudadano in ciudadanos if not ciudadano.vacunado}

def obtener_ambas_dosis(ciudadanos):
    # Supongamos que todos los vacunados tienen ambas dosis
    return {ciudadano for ciudadano in ciudadanos if ciudadano.vacunado}

def obtener_solo_pfizer(ciudadanos, vacunados_pfizer):
    return vacunados_pfizer

def obtener_solo_astrazeneca(ciudadanos, vacunados_astrazeneca):
    return vacunados_astrazeneca

def imprimir_ciudadanos(ciudadanos):
    for ciudadano in ciudadanos:
        print(ciudadano)

def main():
    ciudadanos = generar_ciudadanos(500)
    vacunados_pfizer = generar_vacunados("Pfizer", 75)
    vacunados_astrazeneca = generar_vacunados("AstraZeneca", 75)

    # Operaciones de teor√≠a de conjuntos
    no_vacunados = obtener_no_vacunados(ciudadanos)
    ambas_dosis = obtener_ambas_dosis(ciudadanos)
    solo_pfizer = obtener_solo_pfizer(ciudadanos, vacunados_pfizer)
    solo_astrazeneca = obtener_solo_astrazeneca(ciudadanos, vacunados_astrazeneca)

    # Imprimir resultados
    print("Ciudadanos no vacunados:")
    imprimir_ciudadanos(no_vacunados)

    print("\nCiudadanos con ambas dosis:")
    imprimir_ciudadanos(ambas_dosis)

    print("\nCiudadanos solo con Pfizer:")
    imprimir_ciudadanos(solo_pfizer)

    print("\nCiudadanos solo con AstraZeneca:")
    imprimir_ciudadanos(solo_astrazeneca)

if __name__ == "__main__":
    main()
